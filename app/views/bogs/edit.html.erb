<form action= "/bogs/<% @creature.id %>" method= "post" >
	<input type="hidden" name "_method" value="put">
	<input type= "text" name= "creature[name]" value="<%= @creature.name %>" > <br>
	<textarea name= "creature[description]" value="<%= @creature.description %>"></textarea><br>
	<%= token_tag form_authenticity_token %>

	<button>REBOG</button>
</form>
<a href="/">Main Page</a>